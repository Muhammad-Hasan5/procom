{
	"compilerOptions": {
		"target": "es2020",
		"module": "esnext",
		"moduleResolution": "Bundler",
		"outDir": "dist",
		"rootDir": "src",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true
	},
	"include": ["src"]
}
